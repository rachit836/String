#include <iostream>
using namespace std;

int main() {
    char str[50];
    cout<<"enter the value of string "<<endl;
    cin>>str;
    char*ptr=str;
    
    cout<<"string : ";
    while(*ptr !='\0'){
        cout<<*ptr;
        ptr++;
    }
  
}

OUTPUT:
enter the value of string 
rachit
string : rachit
